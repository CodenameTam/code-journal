<%- include("partials/header"); -%>
    <h1>Home</h1>
    <p> <%= startingContent %> </p>


<div class="allPosts">
    <% posts.forEach(function(post){ %> <!-- How to get the posts inputs-->
     <div class="postContainer">
      <h2> <%= post.title %> </h2> 
      <h4><%= post.date %> </h4>
     <p><%= post.content.substring(0, 100) + "..."%> 
      <a href="/posts/<%= post._id%>">Read More</a>
    </p>
    </div>
  
     
   <% }) %>
  </div>

<%- include("partials/footer"); -%>